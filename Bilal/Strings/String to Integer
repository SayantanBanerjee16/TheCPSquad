class Solution {
public:
    int myAtoi(string str) {
        long res=0;
        
        stringstream s(str);
        s>>res;
        
        int x = res;
        cout<<x<<res;
        if(x!=res && res>0) return INT_MAX;
        else if (x!=res && res<0) return INT_MIN;
        
        
        return res;
        
     }
};
